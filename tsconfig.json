{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node", "hardhat"]
  },
  "include": ["hardhat.config.ts", "scripts/**/*.ts", "workflows/**/*.ts", "typechain-types/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}



